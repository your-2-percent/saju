{
  "name": "@saju/shared",
  "type": "module",
  "private": true,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup",
    "lint": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@saju/tsconfig": "workspace:*",
    "@saju/eslint-config": "workspace:*",
    "eslint": "catalog:"
  }
}